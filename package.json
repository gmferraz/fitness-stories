{
  "name": "fitness-stories",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "build:ios:production": "eas build -p ios --profile production --local",
    "build:ios:development": "eas build -p ios --profile development",
    "build:android:production": "eas build -p android --profile production --local",
    "run:android": "expo prebuild && SENTRY_DISABLE_AUTO_UPLOAD=true bun android",
    "run:ios": "expo prebuild && SENTRY_DISABLE_AUTO_UPLOAD=true bun ios"
  },
